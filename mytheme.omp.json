{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "upgrade": {
    "notice": true,
    "interval": "8h",
    "auto": false,
    "source": "cdn"
  },
  "console_title_template": "{{ if .Root }}root {{ end }}{{ .Shell }} in {{ .Folder }}",
  "shell_integration": true,
  "final_space": true,
  "palette": {
    "error-bg": "#800000",
    "admin-bg": "#800000",
    "dark-red": "#700000",
    "green": "#008000",
    "dark-green": "#006000",
    "dark-blue": "#000070",
    "orange": "#FF7F00",
    "dark-orange": "#B05000",
    "yellow": "#FFFF00",
    "dark-yellow": "#ffc329",
    "brown": "#803000",
    "teal": "#008080",
    "dark-teal": "#004040",
    "chalk": "#E0E0E0",
    "dark-lavender": "#734F96",
    "battery": "p:dark-green",
    "battery-foreground": "p:chalk",
    "battery-charging": "blue",
    "battery-full": "p:dark-green",
    "battery-discharging": "p:dark-orange",
    "firebase-fg": "p:chalk",
    "firebase-bg": "#F0A000",
    "git-foreground": "p:chalk",
    "git": "p:dark-green",
    "git-modified": "p:green",
    "git-diverged": "p:dark-orange",
    "git-ahead": "p:dark-lavender",
    "git-behind": "p:dark-lavender",
    "go-bg": "#14677D",
    "go-fg": "p:chalk",
    "java-bg": "#4063D8",
    "kotlin-bg": "#906cff",
    "python-fg": "#FFE873",
    "python-bg": "#306998",
    "npm-fg": "#ffc329",
    "npm-bg": "#711418",
    "node-bg": "#6CA35E",
    "exec-time": "#406080",
    "ram-fg": "p:chalk",
    "ram-bg": "p:dark-lavender",
    "rust-fg": "#193459",
    "rust-bg": "#99908a",
    "upgrade-fg": "#111111",
    "upgrade-bg": "#e5b15a"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0d7",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-orange",
          "template": " {{ if .SSHSession }}\udb81\udc84{{ end }} ",
          "cache": {
            "duration": "1h",
            "strategy": "session"
          }
        },
        {
          "type": "text",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0d7",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-orange",
          "template": " {{ if .WSL }}WSL{{ end }} ",
          "cache": {
            "duration": "1h",
            "strategy": "session"
          }
        },
        {
          "type": "os",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0d7",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-green",
          "template": " {{ .Icon }} ",
          "cache": {
            "duration": "1h",
            "strategy": "session"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-blue",
          "background_templates": [
            "{{ if .Root }}p:admin-bg{{ end }}"
          ],
          "cache": {
            "duration": "1h",
            "strategy": "session"
          },
          "template": " {{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-teal",
          "template": " {{ .StackCount }} ",
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:brown",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{.Version}} {{ end }}{{ if .Name }}{{- .Name -}}{{ end }}{{ end }} ",
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "firebase",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:firebase-fg",
          "background": "p:firebase-bg",
          "template": " \udb82\udd67 {{ .Project }}",
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "flutter",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "blue",
          "template": " \ue28e {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "dart",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "blue",
          "template": " \ue798 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:python-fg",
          "background": "p:python-bg",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h",
            "fetch_virtual_env": true,
            "display_mode": "context"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "kotlin",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "p:kotlin-bg",
          "template": " \ue634 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "p:java-bg",
          "template": " \ue738 {{ .Full }}",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:go-fg",
          "background": "p:go-bg",
          "template": " \ue724 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:rust-fg",
          "background": "p:rust-bg",
          "template": " \ue7a8 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:node-bg",
          "template": " \udb80\udf99 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:npm-fg",
          "background": "p:npm-bg",
          "template": " \ue71e {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "cache_duration": "12h"
          },
          "cache": {
            "duration": "1h",
            "strategy": "folder"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-blue",
          "template": "{{ $segment := .Segments.Git }}{{ $icon := \"\uf1d2\" }}{{ if $segment.Dir }} {{ $icon }} {{ $segment.RepoName }}{{ if $segment.IsWorkTree}} \ue21c{{ end }}{{if ne $segment.RepoName .Path}} \ueaf7 {{ trimPrefix $segment.Dir .PWD }}{{ end }}{{ else }} \uea83 {{ path .Path .Location }}{{ end }}{{ if not .Writable }} \uf023{{ end }} ",
          "cache": {
            "duration": "1m",
            "strategy": "folder"
          },
          "properties": {
            "style": "powerlevel",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "max_depth": 4,
            "max_width": 70,
            "mixed_threshold": 4
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:git-foreground",
          "background": "p:git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "source": "cli",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "p:chalk",
          "background": "p:exec-time",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "p:ram-fg",
          "background": "p:ram-bg",
          "template": " \ue266 {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "p:chalk",
          "background": "p:dark-teal",
          "template": " {{ .CurrentDate | date .Format }} "
        },
        {
          "type": "battery",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0b2",
          "powerline_symbol": "\ue0d6",
          "foreground": "p:battery-foreground",
          "background": "p:battery",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:battery-charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:battery-discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:battery-full{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ else }}{{ .Error }}{{ end }} ",
          "properties": {
            "discharging_icon": "\udb84\udea2",
            "charging_icon": "\udb84\udea5",
            "charged_icon": "\udb84\udea6",
            "not_charging_icon": "\udb84\udea3"
          }
        },
        {
          "type": "upgrade",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0b2",
          "powerline_symbol": "\ue0d6",
          "foreground": "p:upgrade-fg",
          "background": "p:upgrade-bg",
          "template": " \uf019 ",
          "cache": {
            "duration": "1m",
            "strategy": "session"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0d7",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-blue",
          "template": " {{ .PromptCount }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:chalk",
          "background": "p:dark-teal",
          "background_templates": [
            "{{ if ne .Code 0 }}p:error-bg{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "status_separator": "|",
            "status_template": "{{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{ .Code | hresult }} {{ reason .Code }}{{ end }}"
          }
        }
      ]
    }
  ]
}
