{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "palette": {
    "error-bg": "#800000",
    "admin-bg": "#800000",
    "dark-red": "#700000",
    "green": "#008000",
    "dark-green": "#006000",
    "dark-blue": "#000070",
    "orange": "#FF7F00",
    "dark-orange": "#B05000",
    "yellow": "#FFFF00",
    "dark-yellow": "#ffc329",
    "teal": "#008080",
    "dark-teal": "#004040",
    "battery": "p:dark-green",
    "battery-foreground": "white",
    "battery-charging": "blue",
    "battery-full": "p:dark-green",
    "battery-discharging": "p:dark-orange",
    "git-foreground": "white",
    "git": "p:dark-green",
    "git-modified": "p:green",
    "git-diverged": "p:dark-orange",
    "git-ahead": "#7040A0",
    "git-behind": "#7040A0",
    "python-fg": "#FFE873",
    "python-bg": "#306998",
    "npm-fg": "#ffc329",
    "npm-bg": "#711418",
    "exec-time": "#406080"
  },
  "final_space": true,
  "shell_integration": true,
  "console_title_template": "{{ if .Root }}root {{ end }}{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-orange",
          "template": " {{ if .SSHSession }}\udb81\udc84{{ end }} "
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-orange",
          "template": " {{ if .WSL }}WSL{{ end }} "
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-green",
          "template": " {{ .Icon }} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-blue",
          "background_templates": [
            "{{ if .Root }}p:admin-bg{{ end }}"
          ],
          "template": " {{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-teal",
          "template": " {{ .StackCount }} "
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "black",
          "background": "p:dark-yellow",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        },
        {
          "type": "flutter",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "blue",
          "template": " \ue28e {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "dart",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "blue",
          "template": " \ue798 {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:python-fg",
          "background": "p:python-bg",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "kotlin",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "#906cff",
          "template": " \ue634 {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:yellow",
          "background": "#4063D8",
          "template": " \ue738 {{ .Full }}",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "template": " \ue724 {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#99908a",
          "template": " \ue7a8 {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "ruby",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "p:dark-red",
          "template": " \ue739 {{ .Full }}",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:npm-fg",
          "background": "p:npm-bg",
          "template": " \ue71e {{ .Full }} ",
          "properties": {
            "cache_version": true
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-blue",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "agnoster_short",
            "max_depth": 4
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:git-foreground",
          "background": "p:git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "white",
          "background": "p:exec-time",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "invert_powerline": true,
          "foreground": "white",
          "background": "p:dark-teal",
          "powerline_symbol": "\ue0b2",
          "template": " {{ .CurrentDate | date .Format }} "
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "p:battery-foreground",
          "background": "p:battery",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:battery-charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:battery-discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:battery-full{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}% ",
          "properties": {
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          }
        },
        {
          "type": "upgrade",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#111111",
          "background": "#FFD664",
          "template": " \uf019 ",
          "properties": {
            "cache_timeout": 1320
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-blue",
          "template": " {{ .PromptCount }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "p:dark-teal",
          "background_templates": [
            "{{ if .Error }}p:error-bg{{ end }}"
          ],
          "template": " {{ .Code }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ]
}